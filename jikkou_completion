  local curr_word=${COMP_WORDS[COMP_CWORD]}
  local prev_word=${COMP_WORDS[COMP_CWORD-1]}

  local commands=""
  local flag_opts="-h --help -V --version"
  local arg_opts="--files -f --file-name -n --values-files --set-label -l --set-annotation --set-value -v --selector -s --output -o --options"
  local format_option_args=("JSON" "YAML") # --output values

  type compopt &>/dev/null && compopt +o default

  case ${prev_word} in
    --files|-f)
      return
      ;;
    --file-name|-n)
      return
      ;;
    --values-files)
      return
      ;;
    --set-label|-l)
      return
      ;;
    --set-annotation)
      return
      ;;
    --set-value|-v)
      return
      ;;
    --selector|-s)
      return
      ;;
    --output|-o)
      local IFS=$'\n'
      COMPREPLY=( $( compReplyArray "${format_option_args[@]}" ) )
      return $?
      ;;
    --options)
      return
      ;;
  esac

  if [[ "${curr_word}" == -* ]]; then
    COMPREPLY=( $(compgen -W "${flag_opts} ${arg_opts}" -- "${curr_word}") )
  else
    local positionals=""
    local IFS=$'\n'
    COMPREPLY=( $(compgen -W "${commands// /$'\n'}${IFS}${positionals}" -- "${curr_word}") )
  fi
}

