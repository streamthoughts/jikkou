# Generates completions for the options and subcommands of the `current-context` subcommand.
function _picocli_jikkou_config_currentcontext() {
  # Get completion data
  local curr_word=${COMP_WORDS[COMP_CWORD]}

  local commands=""
  local flag_opts=""
  local arg_opts=""

